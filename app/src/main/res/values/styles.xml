    <!-- Base application theme. -->
    <style name="AppTheme" parent="AppTheme.Base">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:autofilledHighlight" tools:ignore="NewApi">@android:color/transparent
        </item>
        <item name="colorControlHighlight">@color/colorAccent</item>

        <item name="fontFamily">@font/helvetica_light</item>

        <item name="buttonStyle">@style/buttonStyle</item>
        <item name="colorControlNormal">@color/colorAccent</item>
        <item name="colorControlActivated">@color/colorAccent</item>
        <!--custom-->
        <item name="dialogBackground">@style/dialogBackground</item>
        <item name="dialogTitle">@style/dialogTitle</item>
        <item name="dialogMessage">@style/dialogMessage</item>

    </style>

    <style name="AppTheme.TimePicker" parent="AlertDialog.AppCompat.Light">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="colorControlNormal">@color/colorPrimary</item>
        <item name="android:autofilledHighlight" tools:ignore="NewApi">@android:color/transparent
        </item>
        <item name="colorControlActivated">@color/colorPrimary</item>
        <item name="colorControlHighlight">@color/colorPrimary</item>
    </style>

    <style name="AppTheme.Base" parent="Base.Theme.AppCompat.Light.DarkActionBar">
        <item name="popup">@style/popup</item>
        <item name="toolbar">@style/toolbar</item>
        <item name="android:autofilledHighlight" tools:ignore="NewApi">@android:color/transparent
        </item>
        <item name="appbar">@style/appbar</item>
    </style>

    <style name="AppTheme.NoActionBar" parent="AppTheme">
        <item name="windowActionBar">false</item>
        <item name="android:autofilledHighlight" tools:ignore="NewApi">@android:color/transparent</item>
        <item name="windowNoTitle">true</item>
        <item name="colorControlNormal">@color/colorAccent</item>
        <item name="colorControlActivated">@color/colorAccent</item>
        <item name="colorControlHighlight">@color/colorAccent</item>
    </style>


    <style name="AppTheme.Dashboard" parent="AppTheme.NoActionBar">
        <item name="android:autofilledHighlight" tools:ignore="NewApi">@android:color/transparent
        </item>
        <item name="android:windowTranslucentStatus" tools:targetApi="kitkat">false</item>
        <item name="android:windowDrawsSystemBarBackgrounds" tools:targetApi="lollipop">true</item>
        <item name="android:statusBarColor" tools:targetApi="lollipop">@android:color/transparent
        </item>
    </style>

    <style name="AppTheme.Dashboard.Black" parent="AppTheme.NoActionBar">
        <item name="colorPrimary">@android:color/transparent</item>
        <item name="android:autofilledHighlight" tools:ignore="NewApi">@android:color/transparent</item>
        <item name="colorPrimaryDark">@android:color/transparent</item>
        <item name="android:statusBarColor" tools:targetApi="lollipop">@color/colorPrimary</item>
    </style>

    <style name="AppTheme.Login" parent="AppTheme.NoActionBar">
        <item name="android:autofilledHighlight" tools:ignore="NewApi">@android:color/transparent</item>
        <item name="colorPrimary">@color/colorAccent</item>
        <item name="colorPrimaryDark">@color/colorAccent</item>
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="colorControlNormal">@color/colorAccent</item>
        <item name="colorControlActivated">@color/colorAccent</item>
        <item name="colorControlHighlight">@color/colorAccent</item>
    </style>
    <style name="AppTheme.Splash" parent="AppTheme.NoActionBar">
        <item name="android:autofilledHighlight" tools:ignore="NewApi">@android:color/transparent
        </item>
    </style>

    <style name="AppTheme.Dark" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@android:color/transparent</item>
        <item name="android:autofilledHighlight" tools:ignore="NewApi">@android:color/transparent
        </item>
        <item name="colorPrimaryDark">@color/colorAccent</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorControlNormal">@color/colorAccent</item>
        <item name="colorControlActivated">@color/colorGray</item>
        <item name="colorControlHighlight">@color/colorGray</item>
        <item name="android:windowBackground">@color/colorAccentWhite</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.Black" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorBlack</item>
        <item name="android:autofilledHighlight" tools:ignore="NewApi">@android:color/transparent
        </item>
        <item name="colorPrimaryDark">@color/colorBlack</item>
        <item name="colorAccent">@color/colorBlack</item>
        <item name="colorControlNormal">@color/colorBlack</item>
        <item name="colorControlActivated">@color/colorBlack</item>
        <item name="colorControlHighlight">@color/colorBlack</item>
        <item name="android:windowBackground">@color/colorBlack</item>
    </style>

    <style name="ActivityTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@android:color/transparent</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorControlNormal">@color/colorPrimaryDark</item>
        <item name="android:windowEnableSplitTouch">true</item>
        <item name="android:autofilledHighlight" tools:ignore="NewApi">@android:color/transparent
        </item>
        <item name="android:statusBarColor" tools:targetApi="lollipop">@android:color/transparent
        </item>
        <item name="android:windowTranslucentStatus" tools:targetApi="kitkat">true</item>
        <item name="android:windowTranslucentNavigation" tools:targetApi="kitkat">true</item>
        <item name="android:windowLightStatusBar" tools:targetApi="m">false</item>

    </style>

    <style name="AppTheme.AppBarOverlay" parent="ActivityTheme" />

    <style name="AppBarOverlay" parent="AppTheme.Dark" />


    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="AlertDialogTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="buttonBarNegativeButtonStyle">@style/NegativeButtonStyle</item>
        <item name="buttonBarPositiveButtonStyle">@style/PositiveButtonStyle</item>
    </style>

    <style name="CheckboxStyle" parent="AppTheme">
        <item name="colorAccent">@color/colorPrimary</item>
        // for checked
        <item name="android:textColorSecondary">@android:color/darker_gray</item>
        // for unchecked
    </style>

    <style name="NegativeButtonStyle" parent="Widget.AppCompat.Button.ButtonBar.AlertDialog">
        <item name="android:textColor">#f00</item>
    </style>

    <style name="PositiveButtonStyle" parent="Widget.AppCompat.Button.ButtonBar.AlertDialog">
        <item name="android:textColor">#00f</item>
    </style>

    <style name="whiteProgress" parent="ThemeOverlay.AppCompat.Light">
        <item name="colorAccent">#fff</item>
    </style>


    <!--migrate from merchant-->

    <style name="AppCompatAlertDialogStyle" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="fontFamily">@font/helvetica_light</item>
    </style>

    <style name="toolbar" parent="@style/ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="appbar" parent="@style/ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="popup" parent="@style/ThemeOverlay.AppCompat.Light" />

    <!--navigation-->
    <style name="dialogBackground">
        <item name="android:background">@drawable/rectangle_bg_dialog</item>
        <item name="android:paddingTop">@dimen/activity_vertical_margin</item>
        <item name="android:paddingBottom">@dimen/activity_vertical_margin_double</item>
        <item name="android:paddingLeft">@dimen/activity_horizontal_margin</item>
        <item name="android:paddingRight">@dimen/activity_horizontal_margin</item>
        <item name="android:layout_marginTop">@dimen/activity_vertical_margin</item>
        <item name="android:layout_marginBottom">@dimen/activity_vertical_margin</item>
        <item name="android:layout_marginLeft">@dimen/activity_horizontal_margin</item>
        <item name="android:layout_marginRight">@dimen/activity_horizontal_margin</item>
    </style>


    <style name="tvNavTitle">
        <item name="android:textSize">@dimen/tvNavTitle</item>
        <item name="android:textColor">@color/tvDark</item>
        <item name="fontFamily">@font/helvetica_medium</item>
        <item name="android:ellipsize">end</item>
        <item name="android:paddingLeft">3dp</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:paddingTop">5dp</item>
    </style>

    <style name="tvNavValue">
        <item name="android:textSize">@dimen/tvNavValue</item>
        <item name="android:textColor">@color/tv</item>
        <item name="fontFamily">@font/helvetica_light</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingLeft">3dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="tvNavigationChoice">
        <item name="fontFamily">@font/helvetica_light</item>
        <item name="android:textSize">@dimen/tvNavigation</item>
        <item name="android:button">@null</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:textColor">@drawable/selector_nav_home</item>
        <item name="android:background">@drawable/line_up_down_clickable</item>
        <item name="android:drawablePadding">10dp</item>
    </style>

    <style name="tvToolbar" parent="Base.TextAppearance.Widget.AppCompat.Toolbar.Title">
        <item name="android:textSize">@dimen/tvTitle</item>
        <item name="android:textColor">@color/colorWhite</item>
        <item name="fontFamily">@font/helvetica_light</item>
    </style>

    <style name="dialogTitle">
        <item name="android:textColor">@color/textColorTitle</item>
        <item name="android:textSize">@dimen/tvTitle</item>
        <item name="android:paddingBottom">8dp</item>
        <item name="fontFamily">@font/helvetica_regular</item>
    </style>

    <style name="dialogMessage">
        <item name="android:textColor">@color/textColorMessage</item>
        <item name="android:textSize">@dimen/tvMessage</item>
    </style>


    <style name="buttonStyle" parent="Base.Widget.AppCompat.Button">
        <item name="fontFamily">@font/helvetica_semi_bold</item>
        <item name="android:background">@drawable/bg_button</item>
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:minWidth">100dp</item>
        <item name="android:textSize">@dimen/tvButton</item>
    </style>

    <!--dashboard-->
    <style name="tvDashboardParam">
        <item name="android:textSize">@dimen/tvParam</item>
        <item name="android:textColor">@color/tvLiteExtra</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="fontFamily">@font/helvetica_light</item>
    </style>

    <style name="cardDashboard">
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/bg_home_item_clickable</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
    </style>

    <style name="cardDashboard_kitkat">
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/bg_home_item</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
    </style>

    <style name="tvValueBig">
        <item name="android:textSize">@dimen/tvValueBig</item>
        <item name="android:textColor">@color/tv</item>
        <item name="fontFamily">@font/helvetica_bold</item>
        <item name="android:paddingLeft">5dp</item>
    </style>

    <style name="tvValueBigDashboard" parent="tvValueBig">
        <item name="android:background">@drawable/bg_value_all_clickable</item>
        <item name="android:minWidth">@dimen/tvValueBigSizeMin</item>
        <item name="android:minHeight">@dimen/tvValueBigSizeMin</item>
        <item name="android:paddingLeft">@dimen/tvValueBigPadding</item>
        <item name="android:paddingRight">@dimen/tvValueBigPadding</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="tvHeadSub">
        <item name="android:textSize">@dimen/tvHeadSub</item>
        <item name="android:textColor">@color/tv</item>
        <item name="fontFamily">@font/helvetica_semi_bold</item>
    </style>

    <style name="tvPie">
        <item name="android:textSize">@dimen/tvValueSmall</item>
        <item name="android:textColor">@color/tv</item>
        <item name="fontFamily">@font/helvetica_light</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:padding">3dp</item>
    </style>

    <style name="pb" parent="@style/Base.Widget.AppCompat.ProgressBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!-- - - -->
    <!-- - - -->
    <!--item -->
    <!-- - - -->
    <!-- - - -->
    <style name="tvListError">
        <item name="android:textSize">@dimen/tvMessage</item>
        <item name="android:textColor">@color/tv</item>
        <item name="fontFamily">@font/helvetica_light</item>
    </style>

    <style name="tvListErrorMore">
        <item name="android:textSize">@dimen/tvMessage</item>
        <item name="android:textColor">@color/tv</item>
        <item name="fontFamily">@font/helvetica_light</item>
    </style>

    <style name="tvListNoItem">
        <item name="android:textSize">@dimen/tvMessage</item>
        <item name="android:textColor">@color/tv</item>
        <item name="fontFamily">@font/helvetica_light</item>
    </style>

    <style name="et" parent="Base.Widget.AppCompat.EditText">
        <item name="colorControlNormal">@color/colorNormal</item>
        <item name="colorControlActivated">@color/colorActivated</item>
        <item name="android:textColor">@color/tv</item>
        <item name="android:textSize">@dimen/etText</item>
        <item name="fontFamily">@font/helvetica_light</item>
    </style>

    <style name="etNonEditable" parent="et">
        <item name="android:focusableInTouchMode">false</item>
        <item name="android:focusable">false</item>
        <item name="android:editable">false</item>
        <item name="android:inputType">none</item>
        <item name="android:clickable">true</item>
    </style>

    <style name="tvSnackbar">
        <item name="android:textSize">@dimen/tvSnackBar</item>
        <item name="android:textColor">@color/colorWhite</item>
        <item name="fontFamily">@font/helvetica_light</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:paddingBottom">15dp</item>
        <item name="android:background">#323232</item>
    </style>

    <style name="buttonStyleTrans" parent="buttonStyle">
        <item name="fontFamily">@font/helvetica_light</item>
        <item name="android:textSize">@dimen/tvButton</item>
        <item name="android:textStyle">normal</item>
        <item name="android:background">?attr/selectableItemBackground</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:textColor">@color/tvDark</item>
    </style>

    <style name="tvPrice">
        <item name="android:textSize">@dimen/tvPrice</item>
        <item name="android:textColor">@color/colorAccent</item>
        <item name="fontFamily">@font/helvetica_light</item>
    </style>

    <style name="tvParam">
        <item name="android:textSize">@dimen/tvParam</item>
        <item name="android:textColor">@color/tvDark</item>
        <item name="fontFamily">@font/helvetica_regular</item>
    </style>

    <style name="tvListValue">
        <item name="android:paddingTop">@dimen/tvListVerticalPaddingValue</item>
        <item name="android:paddingBottom">@dimen/tvListVerticalPaddingValue</item>
        <item name="android:paddingLeft">@dimen/tvListHorizontalPaddingValue</item>
        <item name="android:paddingRight">@dimen/tvListHorizontalPaddingValue</item>
        <item name="android:textSize">@dimen/tvListValue</item>
        <item name="android:textColor">@color/tvLite</item>
        <item name="fontFamily">@font/helvetica_medium</item>
    </style>

    <style name="tvListParam">
        <item name="android:paddingTop">@dimen/tvListVerticalPaddingParam</item>
        <item name="android:paddingBottom">@dimen/tvListVerticalPaddingParam</item>
        <item name="android:paddingLeft">@dimen/tvListHorizontalPaddingParam</item>
        <item name="android:paddingRight">@dimen/tvListHorizontalPaddingParam</item>
        <item name="android:textSize">@dimen/tvListParam</item>
        <item name="android:textColor">@color/tvDark</item>
        <item name="fontFamily">@font/helvetica_regular</item>
    </style>

    <style name="formTitle">
        <item name="fontFamily">@font/helvetica_semi_bold</item>
        <item name="android:textColor">@color/textColorTitle</item>
        <item name="android:textSize">@dimen/tvTitle</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="textView" parent="android:Widget.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/helvetica_regular</item>
        <item name="android:textSize">@dimen/login_edit_size</item>
        <item name="android:textColor">@color/edit_txt</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">center|left</item>
    </style>

    <style name="editText" parent="android:Widget.EditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/etText</item>
        <item name="android:fontFamily">@font/helvetica_light</item>
        <item name="android:textColor">@color/edit_txt</item>
        <item name="android:textColorHint">@color/colorRippleBlack</item>
        <item name="android:maxLines">1</item>
        <item name="android:longClickable">false</item>
        <item name="android:singleLine">true</item>
        <item name="android:gravity">center|left</item>
    </style>


    <style name="default_button" parent="android:Widget.Button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:background">@drawable/button_bg</item>
        <item name="android:fontFamily">@font/helvetica_bold</item>
        <item name="android:textSize">@dimen/login_edit_size</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:maxLines">1</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="default_button_square" parent="android:Widget.Button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:background">@color/colorOrange</item>
        <item name="android:fontFamily">@font/helvetica_bold</item>
        <item name="android:textSize">@dimen/login_edit_size</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:maxLines">1</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="default_button_plain" parent="android:Widget.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:background">@color/colorWhite</item>
        <item name="android:fontFamily">@font/helvetica_semi_bold</item>
        <item name="android:textSize">@dimen/login_edit_size</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:maxLines">1</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:textColor">@color/colorOrange</item>
    </style>

    <style name="MyAlertDialogTheme" parent="Theme.AppCompat.Dialog.Alert">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:textColor">@color/colorAccent</item>
        <item name="android:textColorHint">@color/colorAccent</item>
        <item name="android:colorBackground">@color/colorWhite</item>
        <item name="android:windowBackground">@color/colorWhite</item>
        <item name="android:textColorPrimary">@android:color/black</item>
        <item name="android:textColorSecondary">@android:color/black</item>
        <item name="android:titleTextColor" tools:targetApi="m">@android:color/black</item>
    </style>


</resources>
